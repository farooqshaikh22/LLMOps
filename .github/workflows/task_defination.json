{
    "family": "multiple-doc-chat",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::536607082396:role/ecsTaskExecutionRole",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "8192",
    "containerDefinitions": [
      {
        "name": "multiple-doc-chat-container",
        "image": "536607082396.dkr.ecr.us-east-1.amazonaws.com/multiple-doc-chat-repo",
        "cpu": 1024,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp",
            "name": "multiple-doc-chat-container-8080-tcp",
            "appProtocol": "http"
          }
        ],
        "environment": [
          {
            "name": "ENV",
            "value": "production"
          }
        ],
        "secrets": [
        {
            "name": "GROQ_API_KEY",
            "valueFrom": "arn:aws:secretsmanager:us-east-1:536607082396:secret:multidocchatsecret-q0Rsjk:GROQ_API_KEY::"
        },
        {
            "name": "GOOGLE_API_KEY",
            "valueFrom": "arn:aws:secretsmanager:us-east-1:536607082396:secret:multidocchatsecret-q0Rsjk:GOOGLE_API_KEY::"
        },
        {
            "name": "LANGCHAIN_API_KEY",
            "valueFrom": "arn:aws:secretsmanager:us-east-1:536607082396:secret:multidocchatsecret-q0Rsjk:LANGCHAIN_API_KEY::"
        },
        {
            "name": "LANGCHAIN_TRACING_V2",
            "valueFrom": "arn:aws:secretsmanager:us-east-1:536607082396:secret:multidocchatsecret-q0Rsjk:LANGCHAIN_TRACING_V2::"
        },
        {
            "name": "LANGCHAIN_ENDPOINT",
            "valueFrom": "arn:aws:secretsmanager:us-east-1:536607082396:secret:multidocchatsecret-q0Rsjk:LANGCHAIN_ENDPOINT::"
        },
        {
            "name": "LANGCHAIN_PROJECT",
            "valueFrom": "arn:aws:secretsmanager:us-east-1:536607082396:secret:multidocchatsecret-q0Rsjk:LANGCHAIN_PROJECT::"
        }
        ],

        
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/lmultiple-doc-chat",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs",
            "awslogs-create-group": "true"
          }
        }
      }
    ]
  }
  